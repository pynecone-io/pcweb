{
	"image": "mcr.microsoft.com/devcontainers/python:1-3.11-bookworm",
	"postCreateCommand": "/bin/bash -c 'python -m venv venv && ./venv/bin/pip install -r requirements.txt && ./venv/bin/reflex init'",
	"forwardPorts": [3000, 8000],
	"portsAttributes": {
		"3000": {
			"label": "Frontend",
			"onAutoForward": "notify"
		},
		"8000": {
			"label": "Backend"
		}
	}
}
